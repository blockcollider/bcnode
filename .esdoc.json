{
  "source": "./src",
  "destination": "./docs",
  "includes": [
    ".*\\.es6$"
  ],
  "plugins": [
    {
      "name": "esdoc-standard-plugin",
      "option": {
        "lint": {"enable": true},
        "coverage": {"enable": true},
        "typeInference": {"enable": true}
      }
    },
    {
      "name": "esdoc-ecmascript-proposal-plugin",
      "option": {
        "all": true
      }
    },
    {
      "name": "esdoc-flow-type-plugin",
      "option": {
        "enable": true
      }
    }
  ]
}

