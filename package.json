{
  "name": "bcnode",
  "version": "0.5.4",
  "description": "Block Collider with miners and rovers",
  "main": "lib/src/init.js",
  "scripts": {
    "bcnode": "./bin/cli --ws --rovers --ui --node --miner-key ABCDEF",
    "benchmark": "./bin/benchmark",
    "check": "yarn run dist",
    "doc": "./node_modules/.bin/esdoc || true",
    "docker:build": "yarn && ./scripts/version-generate.js && docker build -t blockcollider/bcnode .",
    "flow:watch": "./node_modules/flow-watch/src/flow-watch.js",
    "lint:standard": "standard src/**/*.es6",
    "lint:flow": "flow",
    "proto": "./node_modules/grpc-tools/bin/protoc.js --proto_path=protos --js_out=import_style=commonjs,binary:src/protos protos/*.proto --grpc_out=src/protos --plugin=protoc-gen-grpc=./node_modules/grpc-tools/bin/grpc_node_plugin && yarn run proto-fix",
    "proto-fix": "./scripts/proto-fix.sh",
    "transpile": "babel src --source-maps=true --config-file ./.babelrc --out-dir lib --ignore ./lib/,./node_modules,./.babelrc,./package.json,./npm-debug.log",
    "test": "jest",
    "build-native": "neon build",
    "build": "yarn run lint:flow && yarn run lint:standard && yarn run transpile --copy-files && yarn run webpack",
    "build-windows": "yarn run lint:standard && yarn run transpile --copy-files && yarn run webpack",
    "dist": "yarn && yarn run proto && yarn run build-native && yarn run build && yarn test --ci --coverage && yarn run nsp check --threshold 7 && yarn run outdated && yarn run doc",
    "dist-windows": "yarn && yarn run build-native && yarn test --ci --coverage && yarn run nsp check --threshold 7 && yarn run outdated && yarn run doc",
    "outdated": "yarn outdated || true",
    "pkg": "pkg package.json --out-path=dist",
    "version:generate": "babel-node ./scripts/version-generate.js",
    "watch:lint": "onchange src/**/*.es6 -- yarn run lint:standard",
    "watch:transpile": "onchange src/**/*.es6 -- babel {{changed}} --source-maps=true --config-file ./.babelrc --out-dir lib --ignore lib,node_modules",
    "watch:test": "jest --watch",
    "watch": "concurrently -p name -n 'standard,babel,jest' 'yarn run watch:lint' 'yarn run watch:transpile' 'yarn run watch:test'",
    "webpack": "./node_modules/webpack-cli/bin/webpack.js --config webpack/webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blockcollider/bcnode.git"
  },
  "author": {
    "name": "Patrick McConlogue",
    "email": "pm@blockcollider.org",
    "url": "https://blockcollider.org/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/blockcollider/bcnode/issues"
  },
  "homepage": "https://github.com/blockcollider/bcnode#readme",
  "bin": {
    "benchmark": "bin/benchmark.js",
    "cli": "bin/cli.js"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "ts",
      "tsx",
      "es6"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/*.*",
      "node_modules/jest/node_modules/jest-cli/build/test_scheduler.js",
      "/lib/"
    ],
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/*.(js|es6)"
    ],
    "transform": {
      "^.+\\.(js|es6)$": "babel-jest"
    },
    "collectCoverage": false
  },
  "standard": {
    "globals": [
      "describe",
      "context",
      "beforeAll",
      "beforeEach",
      "afterAll",
      "afterEach",
      "it",
      "test",
      "expect",
      "jest"
    ],
    "parser": "babel-eslint",
    "plugins": [
      "flowtype"
    ]
  },
  "dependencies": {
