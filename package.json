{
  "name": "bcnode",
  "version": "0.8.5",
  "description": "Block Collider JS Network Node with miner and rovers",
  "main": "lib/src/init.js",
  "scripts": {
    "bcnode": "./bin/cli --ws --rovers --ui --node --miner-key MY_PUBLIC_ADDRESS",
    "benchmark": "./bin/benchmark",
    "check": "yarn run dist",
    "compile-parser": "./node_modules/.bin/jison src/script/script.jison -o src/script/script.js && node ./scripts/parse-add-eslint-ignore.js",
    "doc": "./node_modules/.bin/esdoc",
    "docker:build": "yarn run dist && docker build -t blockcollider/bcnode .",
    "fixme": "./node_modules/fixme/bin/cli src/**/*.es6",
    "flow:watch": "./node_modules/flow-watch/src/flow-watch.js",
    "lint:eslint": "yarn run eslint src/**/*.es6",
    "lint:eslint:watch": "onchange 'src/**/*.es6' -i -- yarn lint:eslint",
    "lint:flow": "true",
    "proto": "./node_modules/grpc-tools/bin/protoc.js --proto_path=protos --js_out=import_style=commonjs,binary:src/protos protos/*.proto --grpc_out=src/protos --plugin=protoc-gen-grpc=./node_modules/grpc-tools/bin/grpc_node_plugin && yarn run proto-fix",
    "proto-fix": "./scripts/proto-fix.sh",
    "version:generate": "node ./scripts/version-generate.js",
    "transpile": "babel src --source-maps=true --config-file ./.babelrc --out-dir lib --copy-files --ignore ./lib/,./node_modules,./.babelrc,./package.json,./npm-debug.log",
    "test": "jest",
    "build-native": "neon build",
    "build": "yarn run lint:flow && yarn run lint:eslint && yarn run transpile && yarn run version:generate && yarn run webpack",
    "build-windows": "yarn run lint:eslint && yarn run transpile --copy-files && yarn run version:generate && yarn run webpack",
    "dist": "yarn && yarn run proto && yarn run build && yarn test --ci --coverage && yarn audit || true && yarn run outdated && yarn run doc",
    "dist-windows": "yarn && yarn run build-native && yarn test --ci --coverage && yarn audit || true && yarn run outdated && yarn run doc",
    "outdated": "yarn outdated || true",
    "pkg": "pkg package.json --out-path=dist",
    "watch:lint": "onchange src/**/*.es6 -- yarn run lint:eslint",
    "watch:transpile": "onchange src/**/*.es6 -- babel {{changed}} --source-maps=true --config-file ./.babelrc --out-dir lib --ignore lib,node_modules",
    "watch:test": "jest --watch",
    "watch": "concurrently -p name -n 'standard,babel,jest' 'yarn run watch:lint' 'yarn run watch:transpile' 'yarn run watch:test'",
    "webpack": "./node_modules/webpack-cli/bin/webpack.js --config webpack/webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blockcollider/bcnode.git"
  },
  "author": {
    "name": "Patrick McConlogue",
    "email": "pm@blockcollider.org",
    "url": "https://www.blockcollider.org/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/blockcollider/bcnode/issues"
  },
  "homepage": "https://github.com/blockcollider/bcnode#readme",
  "bin": {
    "cli": "lib/cli/main.js"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "ts",
      "tsx",
      "es6"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/*.*",
      "node_modules/jest/node_modules/jest-cli/build/test_scheduler.js",
      "/lib/"
    ],
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/*.(js|es6)"
    ],
    "transform": {
      "^.+\\.(js|jsx|es6)$": "babel-jest"
    },
    "collectCoverage": false
  },
  "standard": {
    "globals": [
      "describe",
      "context",
      "beforeAll",
      "beforeEach",
      "afterAll",
      "afterEach",
      "it",
      "test",
      "expect",
      "jest",
      "location",
      "TimeoutID",
      "IntervalID",
      "$Values",
      "$Keys"
    ],
    "parser": "babel-eslint",
    "plugins": [
      "flowtype"
    ]
  },
